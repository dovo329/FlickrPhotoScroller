source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!

target 'FlickrPhotoScroller' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FlickrPhotoScroller
  pod 'Alamofire', '4.5.1'
  pod 'AlamofireImage', '3.3.0'
  pod 'SwiftyJSON', '4.0.0'

  target 'FlickrPhotoScrollerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FlickrPhotoScrollerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
